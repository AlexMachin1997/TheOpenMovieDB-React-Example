{
	"name": "theopenmoviedb-react-example",
	"private": true,
	"version": "0.2.0",
	"main": "index.js",
	"description": "A Progressive Web Application (PWA) to allow users to quickly search for movies, tv shows and various celebrities.",
	"author": "Alex Machin",
	"license": "MIT",
	"keywords": [
		"Testing Library",
		"Jest",
		"Creact React App",
		"Apollo",
		"GraphQL",
		"PWA",
		"Styled-Components",
		"Workbox",
		"Git",
		"Github",
		"SPA",
		"NodeJS",
		"NPM",
		"React-hooks",
		"Eslint",
		"Prettier"
	],
	"bugs": {
		"url": "https://github.com/AlexMachin1997/TheOpenMovieDB-React-Example/issues"
	},
	"scripts": {
		"client:start": "react-scripts start",
		"client:test": "react-scripts test --watchAll=false --maxWorkers=50%",
		"client:eject": "react-scripts eject",
		"client:storybook": "start-storybook -p 9009 -s public",
		"client:build-storybook": "build-storybook -s public",
		"client:build": "react-scripts build && yarn client-pwa:build && yarn client-pwa:clean",
		"client-pwa:build": "node ./src/serviceWorker/generateManifest.js",
		"client-pwa:clean": "rimraf  -f build/precache-manifest.*.js && rimraf  -f build/service-worker.js",
		"client:lint": "yarn run eslint src/**/*.js --fix",
		"client:prettier": "prettier --write ./src"
	},
	"dependencies": {
		"@apollo/client": "^3.5.7",
		"@fortawesome/fontawesome-free": "^6.0.0",
		"@tailwindcss/line-clamp": "^0.3.1",
		"@tippyjs/react": "^4.2.6",
		"autoprefixer": "^10.4.2",
		"classnames": "^2.3.1",
		"font-awesome": "^4.7.0",
		"graphql": "^16.2.0",
		"postcss": "^8.4.6",
		"prettier-plugin-tailwindcss": "^0.1.8",
		"prop-types": "^15.8.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-helmet": "^6.1.0",
		"react-router-dom": "6",
		"react-scripts": "^5.0.0",
		"sass": "^1.49.7",
		"styled-components": "^5.1.1",
		"tailwindcss": "^3.0.18",
		"tippy.js": "^6.3.7",
		"workbox-build": "^5.0.0"
	},
	"devDependencies": {
		"@storybook/addon-a11y": "^6.4.18",
		"@storybook/addon-actions": "^6.4.18",
		"@storybook/addon-docs": "^6.4.18",
		"@storybook/addon-knobs": "^6.0.21",
		"@storybook/addons": "^6.4.18",
		"@storybook/builder-webpack5": "^6.4.18",
		"@storybook/manager-webpack5": "^6.4.18",
		"@storybook/preset-create-react-app": "^4.0.0",
		"@storybook/react": "^6.4.18",
		"@testing-library/jest-dom": "^5.16.2",
		"@testing-library/react": "^12.1.2",
		"@testing-library/user-event": "^13.5.0",
		"eslint": "^8.8.0",
		"eslint-config-airbnb": "^19.0.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.25.3",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.27.0",
		"eslint-plugin-react-hooks": "^4.3.0",
		"eslint-plugin-storybook": "^0.5.6",
		"eslint-webpack-plugin": "^3.0.1",
		"jest-axe": "^5.0.1",
		"prettier": "^2.5.1",
		"webpack": "^5.68.0"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AlexMachin1997/TheOpenMovieDB-React-Example.git"
	}
}
