{
	"name": "theopenmoviedb-react-example",
	"private": true,
	"version": "0.1.0",
	"main": "index.js",
	"description": "A Progressive Web Application (PWA) to allow users to quickly search for movies, tv shows and various celebrities.",
	"author": "Alex Machin",
	"license": "ISC",
	"keywords": [
		"Testing Library",
		"Jest",
		"Creact React App",
		"Apollo",
		"GraphQL",
		"PWA",
		"Styled-Components",
		"Workbox",
		"Git",
		"Github",
		"SPA",
		"NodeJS",
		"NPM",
		"React-hooks",
		"Eslint",
		"Prettier"
	],
	"bugs": {
		"url": "https://github.com/AlexMachin1997/TheOpenMovieDB-React-Example/issues"
	},
	"scripts": {
		"start": "react-scripts start",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"storybook": "start-storybook -p 9009 -s public",
		"build-storybook": "build-storybook -s public",
		"build-sw": "node ./src/serviceWorker/generateManifest.js",
		"clean-cra-sw": "rimraf  -f build/precache-manifest.*.js && rimraf  -f build/service-worker.js",
		"build": "react-scripts build && npm run build-sw && npm run clean-cra-sw",
		"PWA": "react-scripts build && npm run build-sw && npm run clean-cra-sw && serve -s build",
		"allDependencies": "npm run coreDependencies && npm run devDependencies",
		"coreDependencies": "npm install",
		"devDependencies": " npm install --only=dev",
		"lint": "eslint --fix src/**/*.js package.json"
	},
	"dependencies": {
		"@apollo/react-hooks": "^3.1.4",
		"apollo-cache-inmemory": "^1.6.5",
		"apollo-client": "^2.6.8",
		"apollo-link": "^1.2.14",
		"apollo-link-context": "^1.0.20",
		"apollo-link-error": "^1.1.13",
		"apollo-link-http": "^1.5.17",
		"graphql": "^15.1.0",
		"graphql-tag": "^2.10.3",
		"lodash": "^4.17.15",
		"prop-types": "^15.7.2",
		"react": "^16.13.0",
		"react-dom": "^16.13.0",
		"react-router": "^5.1.2",
		"react-router-dom": "^5.1.2",
		"react-scripts": "3.4.0",
		"styled-components": "^5.1.1",
		"styled-icons": "^10.2.1",
		"workbox-build": "^5.0.0"
	},
	"devDependencies": {
		"@storybook/addon-a11y": "^5.3.19",
		"@storybook/addon-actions": "^5.3.19",
		"@storybook/addon-docs": "^5.3.19",
		"@storybook/addon-knobs": "^5.3.19",
		"@storybook/addon-links": "^5.3.19",
		"@storybook/addons": "^5.3.19",
		"@storybook/preset-create-react-app": "^2.0.0",
		"@storybook/react": "^5.3.19",
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.5.0",
		"@testing-library/user-event": "^7.2.1",
		"eslint-config-airbnb": "^18.1.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-import": "^2.21.2",
		"eslint-plugin-prettier": "^3.1.3",
		"eslint-plugin-react": "^7.20.0",
		"eslint-plugin-react-hooks": "^4.0.4",
		"jest-axe": "^3.4.0",
		"prettier": "^2.0.5"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
